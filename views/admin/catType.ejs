<div class="container mt-5">
    <h2>catType List</h2>
    <table class="table">
      <thead>
        <tr>
          <th scope="col">#</th>
          <th scope="col">Name</th>
         
          <th scope="col">Active</th>
          <th scope="col">Actions</th>
        </tr>
      </thead>
      <tbody>
        <% CatType.forEach((catType, index) => { %>
          <tr>
            <th scope="row"><%= index + 1 %></th>
            <td><%= catType.name %></td>
           
            <td><%= catType.active ? 'Yes' : 'No' %></td>
            <td>
              <a href="/admin/edit-catType/<%= catType._id %>" class="btn btn-warning btn-sm">Edit</a>
              <a href="/admin/delete-catType/<%= catType._id %>" class="btn btn-danger btn-sm">Delete</a>
            </td>
          </tr>
        <% }); %>
      </tbody>
    </table>
  </div>